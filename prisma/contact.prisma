generator client {
  provider = "prisma-client-js"
  output   = "./generated/contact"
}

datasource contact {
  provider = "sqlite"
  url      = "file:./contact.sqlite"
}

model contact {
  id        String    @id @default(uuid())
  email     String
  name      String
  createdAt DateTime  @default(now())
  updateAt  DateTime?
  deleteAt  DateTime?
}
