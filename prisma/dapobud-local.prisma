generator client {
  provider = "prisma-client-js"
  output   = "./generated/dapobud-local"
}

datasource dapobud_local {
  provider = "sqlite"
  url      = "file:./dapobud-local.sqlite"
}

model synchronized_history {
  id   String   @id @default(uuid())
  time DateTime @default(now())
}

model administrasi_desa {
  kode_desa      String    @id
  kode_kecamatan String
  desa           String
  aktif          Boolean   @default(true)
  created_at     DateTime  @default(now())
  created_by     String
  updated_at     DateTime  @default(now())
  updated_by     String?
  deleted_at     DateTime?
  deleted_by     String?
}

model administrasi_kabupaten {
  kode_kabupaten String    @id
  kode_provinsi  String
  kabupaten      String
  aktif          Boolean   @default(true)
  created_at     DateTime  @default(now())
  created_by     String
  updated_at     DateTime  @default(now())
  updated_by     String?
  deleted_at     DateTime?
  deleted_by     String?
}

model administrasi_kecamatan {
  kode_kecamatan String    @id
  kode_kabupaten String
  kecamatan      String
  aktif          Boolean   @default(true)
  created_at     DateTime  @default(now())
  created_by     String
  updated_at     DateTime  @default(now())
  updated_by     String?
  deleted_at     DateTime?
  deleted_by     String?
}

model administrasi_provinsi {
  kode_provinsi String    @id
  provinsi      String
  aktif         Boolean   @default(true)
  created_at    DateTime  @default(now())
  created_by    String
  updated_at    DateTime  @default(now())
  updated_by    String?
  deleted_at    DateTime?
  deleted_by    String?
}

model cb {
  id           String    @id
  dapobud_id   String    @unique(map: "cb_dapobud_id_unique")
  master_cb_id String
  cb_id        String
  created_at   DateTime  @default(now())
  created_by   String
  updated_at   DateTime  @default(now())
  updated_by   String?
  deleted_at   DateTime?
  deleted_by   String?
  dapobud      dapobud   @relation(fields: [dapobud_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  master_cb    master_cb @relation(fields: [master_cb_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
}

