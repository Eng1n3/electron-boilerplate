{
  "name": "my-electron-app",
  "version": "1.0.0",
  "description": "Contact Management App",
  "main": "main.js",
  "scripts": {
    "install:all": "concurrently \"cd app && npm install\" \"npm install\"",
    "start:dev:win": "concurrently cross-env NODE_ENV=dev && \"cd app && npm run dev\" \"electronmon .\"",
    "start:dev:linux": "concurrently \"cd app && NODE_ENV=dev npm run dev\" \"NODE_ENV=dev electronmon .\"",
    "build:front:dev:win": "cd app && cross-env NODE_ENV=dev && npm run build",
    "build:front:dev:linux": "cd app && NODE_ENV=dev npm run build",
    "build:front": "cd app && npm run build",
    "start": "electron-forge start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "author": "Antifragile Code",
  "license": "ISC",
  "devDependencies": {
    "@electron-forge/cli": "^6.4.2",
    "@electron-forge/maker-deb": "^6.4.2",
    "@electron-forge/maker-rpm": "^6.4.2",
    "@electron-forge/maker-squirrel": "^6.4.2",
    "@electron-forge/maker-zip": "^6.4.2",
    "@electron-forge/plugin-auto-unpack-natives": "^6.4.2",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "electron": "^26.2.2",
    "electronmon": "^2.0.2"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "url": "^0.11.3"
  }
}
